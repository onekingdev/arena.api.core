{
  "name": "arena/arena.api",
  "type": "project",
  "description": "Arena API",
  "keywords": [
    "arena",
    "api"
  ],
  "license": "UNLICENSED",
  "require": {
    "php": "^7.4",
    "ext-gd": "*",
    "ext-zip": "*",
    "aws/aws-sdk-php": "^3.158",
    "bacon/bacon-qr-code": "^2.0",
    "boyhagemann/wave": "^0.1.0@dev",
    "codedungeon/phpunit-result-printer": "^0.26.2",
    "fideloper/proxy": "^4.4",
    "fruitcake/laravel-cors": "^1.0",
    "guzzlehttp/guzzle": "~6.5",
    "knuckleswtf/scribe": "^3.9",
    "kub-at/php-simple-html-dom-parser": "^1.9",
    "laravel/cashier": "^12.4",
    "laravel/framework": "^8.9",
    "laravel/passport": "^10.0",
    "laravel/socialite": "^5.0",
    "laravel/telescope": "^4.1",
    "league/flysystem-aws-s3-v3": "~1.0",
    "league/fractal": "^0.19",
    "php-ffmpeg/php-ffmpeg": "^0.16.0",
    "pragmarx/google2fa-laravel": "^1.4",
    "pusher/pusher-php-server": "^4.1",
    "sentry/sentry-laravel": "^2.5",
    "wapmorgan/media-file": "^0.1.4",
    "wapmorgan/mp3info": "^0.0.5"
  },
  "require-dev": {
    "barryvdh/laravel-ide-helper": "^2.8",
    "facade/ignition": "^2.3",
    "fzaninotto/faker": "^1.9",
    "mockery/mockery": "^1.4",
    "nunomaduro/collision": "^4.2",
    "phpunit/phpunit": "^8.5",
    "ext-exif": "*"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    },
    "classmap": [
      "database/seeders",
      "database/factories"
    ],
    "file": [
      "app/Helpers/Util.php",
      "app/Helpers/Constant.php",
      "app/Helpers/Builder.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  }
}
